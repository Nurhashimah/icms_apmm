<% unless @classroom_id.blank? || @classroom_id.nil? %>	
	<% if @all_students.count > 0 %>
		<% @all_students.each_with_index do |student, index| %>
			<%=index+=1%>)&nbsp;<%= (student.name).upcase %><BR>
		<% end %>
	<% else %>
		No student available for selected class.
	<% end %>
<% else %>
	Please select a class.
<% end %>